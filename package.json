{
  "name": "svelte-ts",
  "version": "1.0.0",
  "devDependencies": {
    "@pyoner/svelte-ts-preprocess": "^1.3.0",
    "@types/chai": "^4.2.12",
    "@types/chart.js": "^2.9.24",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.2",
    "@wessberg/rollup-plugin-ts": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "bulma": "^0.9.1",
    "chai": "^4.2.0",
    "chart.js": "^2.9.3",
    "mocha": "^8.1.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rollup": "^2.28.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-svelte": "^6.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.26.11",
    "svelte": "^3.28.0",
    "svelte-router-spa": "^5.7.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "sirv-cli": "^1.0.6"
  },
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv public",
    "start:dev": "sirv public --dev -s -p 3000",
    "test": "rollup -c && env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register tests/**/*.test.ts",
    "coverage": "nyc -r text -e .ts -x \"tests/*.test.ts\" npm run test"
  }
}
